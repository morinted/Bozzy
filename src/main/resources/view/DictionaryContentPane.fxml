<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<TableView xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bozzy.controllers.DictionaryContentPaneController" fx:id="table" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
  <columns>
      <TableColumn fx:id="strokeCol" styleClass="mono-font" text="Stroke" />
      <TableColumn fx:id="translationCol" styleClass="emoji-field" text="Translation" />
      <TableColumn fx:id="wordCountCol" text="Words" visible="false"/>
      <TableColumn fx:id="chordCountCol" text="Strokes" visible="false"/>
      <TableColumn fx:id="collisionCountCol" text="Collisions" visible="false"/>
      <TableColumn fx:id="dictionaryNameCol" text="Dictionary" />
  </columns>
 <columnResizePolicy>
    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
 </columnResizePolicy>
</TableView>
